/* Connection stages */
static const char* stageNames[STAGE_MAX] = {
	"none",
	"platform initialization",
    "name resolution",
	"RTSP handshake",
	"control stream initialization",
	"video stream initialization",
	"audio stream initialization",
	"input stream initialization",
